<h1>Games</h1>

<ul>
  <% @games.each do |game| %>
    <li><em><%= game.title %></em>, played in <%= game.month_played %></li>
  <% end %>
</ul>

<%= form_for Game.new do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %>

  <%= f.label :month_played %>:
  <%= f.text_field :month_played %>

  <%= f.submit %>
<% end %>
