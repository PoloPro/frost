<h1>Books</h1>

<ul>
  <% @books.each do |book| %>
  <li><em><%= book.title %></em>, <%= book.author %>, read in <%= book.month_finished %></li>
  <% end %>
</ul>

<%= form_for Book.new do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %>

  <%= f.label :author %>:
  <%= f.text_field :author %>

  <%= f.label :month_finished %>:
  <%= f.text_field :month_finished %>

  <%= f.submit %>
<% end %>

<hr>

<div class="container">
  <div id="left-books" class="container" style="float: left">
    <div>Test</div>
    <div>Another</div>
  </div>
  <div id="right-books" class="container" style="float: right">
    <div>Right Test</div>
    <div>Banana</div>
  </div>
</div>


